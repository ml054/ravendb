{
  "compilerOptions": {
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": false,
    "declaration": false,
    "sourceMap": true,
    "target": "ES6", 
      "esModuleInterop": true,
    "skipLibCheck": true,
    "module": "AMD",
    "typeRoots": [
        "./typings/**/*"
    ]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
